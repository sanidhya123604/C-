
#include <iostream>

using namespace std;

int main()
{
    int n,i,j,count=0;
    cout<<"enter the value of n:";
    cin>>n;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
        {
            count = count +1;
            cout <<" "<<count ;
        }
    cout<<endl;    
    }
    


    return 0;
}
